{
	"$schema": "https://biomejs.dev/schemas/1.7.3/schema.json",
	"organizeImports": {
		"enabled": true
	},
	"linter": {
		"enabled": true,
		"rules": {
			"recommended": true
		}
	},
	"formatter": {
		"enabled": true,
		"lineWidth": 100
	},
	"overrides": [{
		"include": ["apps/web/**"],
		"linter": {
		  "rules": {
			"suspicious": {
			  "noConsoleLog": "off"
			}
		  }
		}
	}]
}
